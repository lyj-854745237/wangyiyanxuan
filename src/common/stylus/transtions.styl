.mask-enter{ opacity: 0 !important;}
.mask-enter-active{transition: 1s opacity !important;}
.mask-enter-to{ opacity: 1 !important;}
.mask-leave-active{transition: 1s opacity !important;}
.mask-leave-to{ opacity: 0 !important;}

.contorlRemove-enter{ 
    opacity: 0 !important;
    transform translate3d(24px,0,0) rotate(0deg) !important;
}
.contorlRemove-enter-active{
    transition: 0.5s all linear !important;
}
.contorlRemove-enter-to{ 
    opacity: 1 !important;
    transform translate3d(0,0,0) rotate(-720deg) !important;
}
.contorlRemove-leave-active{
    transition: 0.5s all linear !important;
}
.contorlRemove-leave-to{ 
    opacity: 0 !important;
    transform translate3d(24px,0,0) rotate(0deg) !important;
}
